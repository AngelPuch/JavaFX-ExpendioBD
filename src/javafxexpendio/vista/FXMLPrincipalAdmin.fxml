         <children>
            <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="130.0">
               <children>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../recurso/home_img.png" />
                     </image>
                  </ImageView>
                  <Button mnemonicParsing="false" text="Inicio" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="130.0">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <children>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../recurso/proveedor_img.png" />
                     </image>
                  </ImageView>
                  <Button mnemonicParsing="false" onAction="#btnClicProveedor" text="Proveedor" />
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="130.0">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <children>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../recurso/wine_img.png" />
                     </image>
                  </ImageView>
                  <Button mnemonicParsing="false" onAction="#btnClicProducto" text="Productos" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
